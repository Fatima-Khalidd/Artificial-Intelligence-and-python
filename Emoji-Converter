def emoji_dictionary(message):
    emojis = {
        ":)": "ðŸ˜€",
    ":(": "ðŸ˜ž",
    ":|": "ðŸ˜‘",
    }
    output = ""
    words = message.split()  # Split into words (descriptive variable name)
    for word in words:
        output += emojis.get(word, word) + " "
    return output  # Return the final output string

message = input("> ")
output = emoji_dictionary(message)
print(output)
